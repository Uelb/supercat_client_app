<h1>New channel</h1>

<form id="new-channel" name="channel">
  <div class="field">
    <label for="title"> title:</label>
    <input type="text" name="title" id="title" value="<%= title %>" >
  </div>

  <div class="field">
    <label for="latitude"> latitude:</label>
    <input type="text" name="latitude" id="latitude" value="<%= latitude %>" >
  </div>

  <div class="field">
    <label for="longitude"> longitude:</label>
    <input type="text" name="longitude" id="longitude" value="<%= longitude %>" >
  </div>

  <div class="field">
    <label for="creator_id"> creator_id:</label>
    <input type="text" name="creator_id" id="creator_id" value="<%= creator_id %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Channel" />
  </div>

</form>

<a href="#/index">Back</a>