<h1>New message</h1>

<form id="new-message" name="message">
  <div class="field">
    <label for="body"> body:</label>
    <input type="text" name="body" id="body" value="<%= body %>" >
  </div>

  <div class="field">
    <label for="latitude"> latitude:</label>
    <input type="text" name="latitude" id="latitude" value="<%= latitude %>" >
  </div>

  <div class="field">
    <label for="longitude"> longitude:</label>
    <input type="text" name="longitude" id="longitude" value="<%= longitude %>" >
  </div>

  <div class="field">
    <label for="author_id"> author_id:</label>
    <input type="text" name="author_id" id="author_id" value="<%= author_id %>" >
  </div>

  <div class="field">
    <label for="channel_id"> channel_id:</label>
    <input type="text" name="channel_id" id="channel_id" value="<%= channel_id %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Message" />
  </div>

</form>

